<?php
/**
 * @file
 * Code for the ATP Page feature.
 */

include_once 'atp_page.features.inc';

/**
 * Implementation of hook_ctools_plugin_directory().
 */
function atp_page_ctools_plugin_directory($module, $plugin) {
  if ($module == 'ctools' && !empty($plugin)) {
    return "plugins/$plugin";
  }
}